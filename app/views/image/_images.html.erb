<div class="row">

      <h1 class="my-4 text-center text-lg-left">Photos of </h1>

      <div class="row text-center text-lg-left">
      <% @images.each do |image| %>
        <div class="col-lg-3 col-md-4 col-xs-6" id="image-view">
          <a href="<%= image.image.url %>" class="sb">
            <%= image_tag(image.image.url, size: "300x300", class: "img-thumbnail") %>
          </a>
        </div>
       <%end%>
      </div>

    </div>